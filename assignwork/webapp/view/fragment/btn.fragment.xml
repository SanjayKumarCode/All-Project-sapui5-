<core:FragmentDefinition
	xmlns:main="sap.ui.webc.main"
	xmlns:core="sap.ui.core"	
		xmlns="sap.ui.table"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:u="sap.ui.unified"
		xmlns:m="sap.m"
		height="10%">
	<m:Page
		showHeader="false"
		enableScrolling="false">
		<m:content>
       
			<Table>
				<extension>
					<m:OverflowToolbar style="Clear">
						<m:ToggleButton
							
							text="All Operation Activity(2134)"
							tooltip="Enable / Disable Visibility Menu Entries"
							pressed="{ui>/showVisibilityMenuEntry}"/>
                    


                        <m:ToggleButton
							
							text="Unassigned Operation Activity(2130)"
							tooltip="Enable / Disable Visibility Menu Entries"
							pressed="{ui>/showVisibilityMenuEntry}"/>
						<m:ToolbarSpacer/>
                            	<m:ToggleButton
							
							text="Assign Operators"
							tooltip="Enable / Disable Visibility Menu Entries"
							pressed="{ui>/showVisibilityMenuEntry}"/>
						<m:ToggleButton
							
							text="Edit Assigned Operators"
							tooltip="Enable / Disable Visibility Menu Entries"
							pressed="{ui>/showVisibilityMenuEntry}"/>
						<m:ToggleButton
						text="Unassign All"
						
							tooltip="Enable / Disable Freezing Menu Entries"
							pressed="{ui>/showFreezeMenuEntry}"/>
						<m:ToggleButton
							icon="sap-icon://action-settings"
							tooltip="Enable / Disable Cell Filter"
							pressed="{ui>/enableCellFilter}"/>

					</m:OverflowToolbar>
				</extension>
			</Table>
		</m:content>
	</m:Page>

    </core:FragmentDefinition>




